<%
    if (result) {
        var timing = item.timing,
            dataForChartBar = [],
            dataForChartPie = [];

        [
            { valueField: 'load',       name: 'Load event'},
            { valueField: 'domLoad',    name: 'Ready event'},
            { valueField: 'document',   name: 'Document downloading\n(without resources)'},
            { valueField: 'script',     name: 'Script downloading'},
            { valueField: 'stylesheet', name: 'Stylesheet downloading'},
            { valueField: 'hh',         name: 'Hammerhead downloading'}
        ].forEach(function(timingProp) {
            dataForChartBar.push({
                state: timingProp.name,
                origin: timing[timingProp.valueField].origin,
                proxy: timing[timingProp.valueField].proxy
            });
        });

        [
            { valueField: 'document',   name: 'Document'},
            { valueField: 'script',     name: 'Script'},
            { valueField: 'stylesheet', name: 'Stylesheet'},
            { valueField: 'other',      name: 'Other'},
            { valueField: 'hh',         name: 'Hammerhead'}
        ].forEach(function(timingProp) {
            dataForChartPie.push({
                name: timingProp.name,
                area: timing[timingProp.valueField].proxy
            });
        });
    }
%>
<script>
    var dataForChartBar = <%- JSON.stringify(dataForChartBar) %>,
        dataForChartPie = <%- JSON.stringify(dataForChartPie) %>,
        buildInfo = <%- (JSON.stringify(result.buildInfo) || 'null') %>;
</script>
<h2>Time</h2>
<div class="charts">
    <div id="chartBar"></div>
    <div id="chartPie"></div>
</div>