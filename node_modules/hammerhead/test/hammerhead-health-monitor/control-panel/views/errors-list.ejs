<h2>Errors (<%= item.console.length %>)</h2>
<ul class="list-table errors">
    <li class="list-table-row headers">
        <div class="list-table-cell text">
            <div class="wrapper"><b>Text</b></div>
        </div>
        <div class="list-table-cell stack">
            <div class="wrapper"><b>Stack</b></div>
        </div>
        <div class="list-table-cell more">
            <div class="wrapper"><b>Info</b></div>
        </div>
    </li>
    <% if (item.console.length) { %>
        <% for (var j = 0; j < item.console.length; j++) { %>
            <li class="list-table-row">
                <div class="list-table-cell text">
                    <div class="wrapper"><%= item.console[j].text %></div>
                </div>
                <div class="list-table-cell stack">
                    <div class="wrapper"><%- (item.console[j].stack || 'null') %></div>
                </div>
                <div class="list-table-cell more">
                    <div class="wrapper"><a><span class="more">more</span><span class="less">less</span></a></div>
                </div>
            </li>
        <% } %>
    <% } else { %>
        <li class="list-table-row">
            <div class="list-table-cell empty">
                <div class="wrapper">All is well</div>
            </div>
        </li>
    <% } %>
</ul>
